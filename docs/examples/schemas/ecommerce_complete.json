{
  "name": "ecommerce_complete",
  "description": "Complete e-commerce dataset with customers, products, orders, and order items. Demonstrates foreign key relationships, Faker rules, and realistic data generation.",
  "tables": [
    {
      "name": "customers",
      "target_rows": 1000,
      "columns": [
        {
          "name": "customer_id",
          "data_type": "INT",
          "is_unique": true,
          "required": true
        },
        {
          "name": "email",
          "data_type": "VARCHAR",
          "varchar_length": 100,
          "faker_rule": "email",
          "required": true
        },
        {
          "name": "first_name",
          "data_type": "VARCHAR",
          "varchar_length": 50,
          "faker_rule": "first_name"
        },
        {
          "name": "last_name",
          "data_type": "VARCHAR",
          "varchar_length": 50,
          "faker_rule": "last_name"
        },
        {
          "name": "phone",
          "data_type": "VARCHAR",
          "varchar_length": 20,
          "faker_rule": "phone_number"
        },
        {
          "name": "city",
          "data_type": "VARCHAR",
          "varchar_length": 50,
          "faker_rule": "city"
        },
        {
          "name": "country",
          "data_type": "VARCHAR",
          "varchar_length": 50,
          "faker_rule": "country"
        },
        {
          "name": "signup_date",
          "data_type": "DATE",
          "date_start": "2023-01-01",
          "date_end": "2024-12-31"
        },
        {
          "name": "loyalty_points",
          "data_type": "INT",
          "min_value": 0,
          "max_value": 10000
        },
        {
          "name": "is_active",
          "data_type": "BOOLEAN"
        }
      ]
    },
    {
      "name": "products",
      "target_rows": 500,
      "columns": [
        {
          "name": "product_id",
          "data_type": "INT",
          "is_unique": true,
          "required": true
        },
        {
          "name": "product_name",
          "data_type": "VARCHAR",
          "varchar_length": 100,
          "faker_rule": "word"
        },
        {
          "name": "category",
          "data_type": "VARCHAR",
          "varchar_length": 50
        },
        {
          "name": "price",
          "data_type": "FLOAT",
          "min_value": 9.99,
          "max_value": 999.99
        },
        {
          "name": "cost",
          "data_type": "FLOAT",
          "min_value": 5.0,
          "max_value": 500.0
        },
        {
          "name": "stock_quantity",
          "data_type": "INT",
          "min_value": 0,
          "max_value": 1000
        },
        {
          "name": "rating",
          "data_type": "FLOAT",
          "min_value": 1.0,
          "max_value": 5.0
        },
        {
          "name": "in_stock",
          "data_type": "BOOLEAN"
        }
      ]
    },
    {
      "name": "orders",
      "target_rows": 3000,
      "columns": [
        {
          "name": "order_id",
          "data_type": "INT",
          "is_unique": true,
          "required": true
        },
        {
          "name": "customer_id",
          "data_type": "INT",
          "required": true,
          "foreign_key": {
            "references_table": "customers",
            "references_column": "customer_id"
          }
        },
        {
          "name": "order_date",
          "data_type": "DATE",
          "date_start": "2023-01-01",
          "date_end": "2024-12-31"
        },
        {
          "name": "total_amount",
          "data_type": "FLOAT",
          "min_value": 10.0,
          "max_value": 5000.0
        },
        {
          "name": "shipping_cost",
          "data_type": "FLOAT",
          "min_value": 0.0,
          "max_value": 50.0
        },
        {
          "name": "tax_amount",
          "data_type": "FLOAT",
          "min_value": 0.0,
          "max_value": 500.0
        },
        {
          "name": "status",
          "data_type": "VARCHAR",
          "varchar_length": 20
        },
        {
          "name": "payment_method",
          "data_type": "VARCHAR",
          "varchar_length": 20
        }
      ]
    },
    {
      "name": "order_items",
      "target_rows": 8000,
      "columns": [
        {
          "name": "item_id",
          "data_type": "INT",
          "is_unique": true,
          "required": true
        },
        {
          "name": "order_id",
          "data_type": "INT",
          "required": true,
          "foreign_key": {
            "references_table": "orders",
            "references_column": "order_id"
          }
        },
        {
          "name": "product_id",
          "data_type": "INT",
          "required": true,
          "foreign_key": {
            "references_table": "products",
            "references_column": "product_id"
          }
        },
        {
          "name": "quantity",
          "data_type": "INT",
          "min_value": 1,
          "max_value": 10
        },
        {
          "name": "unit_price",
          "data_type": "FLOAT",
          "min_value": 9.99,
          "max_value": 999.99
        },
        {
          "name": "discount",
          "data_type": "FLOAT",
          "min_value": 0.0,
          "max_value": 0.5
        }
      ]
    }
  ]
}
